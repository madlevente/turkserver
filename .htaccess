SetEnv HTTP_READ_HTACCESS On

<IfModule mod_rewrite.c>

	# Tell PHP that the mod_rewrite module is ENABLED.
	SetEnv HTTP_MOD_REWRITE On

	# RewriteEngine is required.
	# Everything is routed through index.php
	# RewriteBase /<YOUR_USERNAME>/turkserver/ 
	# Comment out the line above and replace the <> part with your username if you're not installing turkserver in the root directory of your server.
	RewriteEngine on
	RewriteCond %{REQUEST_FILENAME} !-f
	RewriteRule ^(.*)$ index.php

</IfModule>

Options -Indexes

